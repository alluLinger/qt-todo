{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-app",
            "type": "shell",
            "group": "build",
            "command": "qmake && make -j5",
            "problemMatcher": "$gcc"
        },
        {
            "label": "build-test",
            "type": "shell",
            "group": "build",
            "command": "qmake test=1 && make -j5",
            "problemMatcher": "$gcc"
        },
    ]
}